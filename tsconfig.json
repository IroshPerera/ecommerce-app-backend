{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "noImplicitAny": false,
    },
    "include": ["src/**/*.ts", "server.ts"],
    "exclude": ["node_modules"]
  }